alias chowner='chown -R 1000:1000 /app/*'
alias envgo='cd /app/django_vue'
alias runserver='envgo; python manage.py runserver --insecure 0.0.0.0:8000'
alias dockmig='findnonode -name "*.pyc" -delete; python manage.py migrate'
alias dockmakemig='findnonode -name "*.pyc" -delete; python manage.py makemigrations'
alias dockschema='./manage.py generateschema  > blog/templates/blog/schema.yml'